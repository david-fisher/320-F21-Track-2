<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.scenebuilder.scenebuilder.BasicFXMLController">
   <children>
      <VBox alignment="CENTER" layoutX="250.0" layoutY="79.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Button fx:id="mainPlay" contentDisplay="CENTER" mnemonicParsing="false" onAction="#switchToSelectionScene" prefHeight="25.0" prefWidth="111.0" text="Play Game" textAlignment="CENTER" />
            <Button fx:id="mainNew" mnemonicParsing="false" onAction="#handleMainNewButtonAction" prefHeight="25.0" prefWidth="128.0" text="New Game" />
            <Button fx:id="mainEdit" mnemonicParsing="false" onAction="#handleMainEditButtonAction" prefHeight="25.0" prefWidth="128.0" text="Edit Game" />
         </children>
      </VBox>
   </children>
</AnchorPane>
