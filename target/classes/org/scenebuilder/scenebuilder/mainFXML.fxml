<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.scenebuilder.scenebuilder.MainFXMLController">
   <center>
      <VBox alignment="CENTER" layoutX="250.0" layoutY="79.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Button fx:id="mainPlay" contentDisplay="CENTER" mnemonicParsing="false" onAction="#playFromMain" prefHeight="50.0" prefWidth="200.0" text="Play Game" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin></Button>
            <Button fx:id="mainNew" mnemonicParsing="false" onAction="#newFromMain" prefHeight="50.0" prefWidth="200.0" text="New Game">
               <font>
                  <Font size="24.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin></Button>
            <Button fx:id="mainEdit" alignment="CENTER" mnemonicParsing="false" onAction="#editFromMain" prefHeight="50.0" prefWidth="200.0" text="Edit Game" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin></Button>
            <Button fx:id="mainEdit1" alignment="CENTER" mnemonicParsing="false" onAction="#exitFromMain" prefHeight="50.0" prefWidth="200.0" text="Exit To Desktop" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </center>
</BorderPane>
